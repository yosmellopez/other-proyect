<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}{% endblock %} - Ican</title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    {% block metas_titulo %} {% endblock %}
    {% block metas_descripcion %} {% endblock %}
    {% block keywords %} {% endblock %}

    {% block metafb %}
        <meta property="og:title" content="Ican"/>
        <meta property="og:type" content="website"/>
        {#<meta property="og:url" content="{{url('homefrontend')}}"/>#}
        <meta property="og:image" content="{{ asset('fb_icon.png') }}"/>
        {#<meta property="og:site_name" content="{{url('homefrontend')}}"/>#}
        <meta property="fb:admins" content=""/>
        <meta property="fb:app_id" content=""/>
        <meta property="og:description" content="Express Computacion"/>
    {% endblock %}

    {% block metatw %}
        <!-- INI Social Twitter -->
        <meta property="twitter:card" content="summary">
        <meta property="twitter:site" content="Ican">
        <meta property="twitter:url" content="">
        <meta property="twitter:title" content="">
        <meta property="twitter:description" content=""/>
        <meta property="twitter:image" content="">
        <!-- END Social Twitter -->
    {% endblock %}

    <link rel="shortcut icon" type="image/x-icon" href="{{ asset('favicon.ico') }}">

    {% block stylesheets %}{% endblock %}
</head>
<body class="{% block bodyclass %}{% endblock bodyclass %}">
{% block body %}


{% endblock %}
</body>
</html>
{% block javascripts %}{% endblock %}
