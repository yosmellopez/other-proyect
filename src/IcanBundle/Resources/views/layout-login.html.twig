{% extends '::base-admin.html.twig' %}

{% block stylesheets %}

    <!--begin::Web font -->
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.16/webfont.js"></script>
    <script>
        WebFont.load({
            google: {"families":["Poppins:300,400,500,600,700","Roboto:300,400,500,600,700"]},
            active: function() {
                sessionStorage.fonts = true;
            }
        });
    </script>
    <!--end::Web font -->

    <!--begin::Global Theme Styles -->
    <link href="{{ asset('bundles/ican/assets/vendors/base/vendors.bundle.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/ican/assets/demo/default/base/style.bundle.css') }}" rel="stylesheet" type="text/css" />

    <!--begin::My Styles -->
    <link href="{{ asset('bundles/ican/css/styles.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/ican/css/my-components.css') }}" rel="stylesheet" type="text/css" />

    {% block stylesheetspecific %}  {% endblock %}
{% endblock %}

{% block bodyclass %} m--skin- m-header--fixed m-header--fixed-mobile m-aside-left--enabled m-aside-left--skin-dark m-aside-left--fixed m-aside-left--offcanvas m-footer--push m-aside--offcanvas-default {% endblock bodyclass %}

{% block body %}

    {% block content %}  {% endblock %}

{% endblock %}        


{% block javascripts %}
    <!--begin::Global Theme Bundle -->
    <script src="{{ asset('bundles/ican/assets/vendors/base/vendors.bundle.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/ican/assets/demo/default/base/scripts.bundle.js') }}" type="text/javascript"></script>
{% block javascriptsspecific %}  {% endblock %}
{% endblock %}


